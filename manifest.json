{
  "manifest_version": 3,
  "name": "Focus Reader Bracket",
  "version": "1.1",
  "description": "Floating reading bracket + page dimming for dyslexia & ADHD",
  "action": {
    "default_title": "Toggle Focus Bracket"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "128": "icon.png"
  }
}